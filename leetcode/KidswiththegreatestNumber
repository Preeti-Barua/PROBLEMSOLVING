import java.util.*;
 import java.util.Collections;
class Solution {
    public List<Boolean> kidsWithCandies(int[] candies, int extraCandies) {
        
        List<Boolean> Ans = new ArrayList<Boolean>();
      
   
        for(int n: candies)
        {
            if(n+extraCandies>max)
            {
                Ans.Add("true");
            }
            else
            {
                Ans.Add("false");
            }
        }
        return Ans;
        
    }
}
